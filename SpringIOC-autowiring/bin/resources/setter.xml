<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>

<bean id = "t" class = "beans.Triangle" autowire = "byName"/>
<bean id = "pointA" class = "beans.Points">
	<property name="x" value="0.0"/>
	<property name="y" value="10.0"/>
</bean>
<bean id = "pointB" class = "beans.Points">
	<property name="x" value="5.0"/>
	<property name="y" value="5.0"/>
</bean>

<bean id = "pointC" class = "beans.Points">
	<property name="x" value="0.0"/>
	<property name="y" value="0.0"/>
</bean> 

</beans>